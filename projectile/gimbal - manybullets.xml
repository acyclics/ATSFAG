        </body>
        <body name="bullet10" pos="0 -0.110 -0.20">
            <geom type="sphere" size="0.008" pos="0 0 0.5" mass="0.08" contype="1" conaffinity="1"/>
            <joint type="slide" pos="0 0 0" axis="1 0 1"/>
            <joint type="slide" pos="0 0 0" axis="0 1 1"/>
            <joint type="slide" pos="0 0 0" axis="0 0 1"/>
        </body>
        <body name="bullet11" pos="0 -0.120 -0.18">
            <geom type="sphere" size="0.008" pos="0 0 0.5" mass="0.08" contype="1" conaffinity="1"/>
            <joint type="slide" pos="0 0 0" axis="1 0 1"/>
            <joint type="slide" pos="0 0 0" axis="0 1 1"/>
            <joint type="slide" pos="0 0 0" axis="0 0 1"/>
        </body>
        
        <body name="target" pos="0 1 0.1">
            <site name="detection" rgba="1 0 0 1" type="cylinder" size="0.1 0.005" euler="90 0 0"/>
            <geom rgba="1 0 0 1" type="cylinder" size="0.1 0.005" euler="90 0 0" contype="1" conaffinity="1" mass="1"/>
            <joint name="target_slidez" type="slide" pos="0 0 0" axis="0 0 1" range="-10.3213 10.3" damping="1.0"/>
            <joint name="target_slidex" type="slide" pos="0 0 0" axis="1 0 0" range="-10.3213 10.3" damping="1.0"/>
        </body>

    </worldbody>

    <actuator>
        <velocity joint="motor1" gear="1" forcelimited="true" forcerange="-10 10" ctrllimited="true" ctrlrange="-20 20"/>
        <velocity joint="motor2" gear="1" forcelimited="true" forcerange="-10 10" ctrllimited="true" ctrlrange="-10 10"/>
        <velocity joint="lFricWheel" gear="1" forcelimited="true" forcerange="99 100"/>
        <velocity joint="rFricWheel" gear="1" forcelimited="true" forcerange="-100 -99"/>
    </actuator>

    <sensor>
        <jointpos name="motor1_encoder" joint="motor1" noise="0.001"/>
        <jointpos name="motor2_encoder" joint="motor2" noise="0.001"/>
        <touch name="bulletDetect" site="detection"/>
    </sensor>

</mujoco>