        <body name="bullet4" pos="0 -0.054 -0.29">
            <geom type="sphere" size="0.008" pos="0 0 0.5" mass="0.08" contype="1" conaffinity="1"/>
            <joint type="slide" pos="0 0 0" axis="1 0 1"/>
            <joint type="slide" pos="0 0 0" axis="0 1 1"/>
            <joint type="slide" pos="0 0 0" axis="0 0 1"/>
        </body>
        <body name="bullet5" pos="0 -0.080 -0.27">
            <geom type="sphere" size="0.008" pos="0 0 0.5" mass="0.08" contype="1" conaffinity="1"/>
            <joint type="slide" pos="0 0 0" axis="1 0 1"/>
            <joint type="slide" pos="0 0 0" axis="0 1 1"/>
            <joint type="slide" pos="0 0 0" axis="0 0 1"/>
        </body>
        <body name="bullet6" pos="0 -0.080 -0.25">
            <geom type="sphere" size="0.008" pos="0 0 0.5" mass="0.08" contype="1" conaffinity="1"/>
            <joint type="slide" pos="0 0 0" axis="1 0 1"/>
            <joint type="slide" pos="0 0 0" axis="0 1 1"/>
            <joint type="slide" pos="0 0 0" axis="0 0 1"/>
        </body>
        <body name="bullet7" pos="0 -0.090 -0.23">
            <geom type="sphere" size="0.008" pos="0 0 0.5" mass="0.08" contype="1" conaffinity="1"/>
            <joint type="slide" pos="0 0 0" axis="1 0 1"/>
            <joint type="slide" pos="0 0 0" axis="0 1 1"/>
            <joint type="slide" pos="0 0 0" axis="0 0 1"/>
        </body>
        <body name="bullet8" pos="0 -0.095 -0.22">
            <geom type="sphere" size="0.008" pos="0 0 0.5" mass="0.08" contype="1" conaffinity="1"/>
            <joint type="slide" pos="0 0 0" axis="1 0 1"/>
            <joint type="slide" pos="0 0 0" axis="0 1 1"/>
            <joint type="slide" pos="0 0 0" axis="0 0 1"/>
        </body>
        <body name="bullet9" pos="0 -0.105 -0.21">
